          "layer": 50
        }
      },
      {
      "sales_banner@start.store_button_sale_banner": {
          "texture": "textures/ui/SaleBanner",
          "$title_text_color": [ 0.82, 1, 0.64 ],
          "anchor_from": "top_right",
          "anchor_to": "top_left",
          "offset": [ -12, -5 ],
          "layer": 60
          }
      },


      {
        "Left_Default": {
          "type": "image",
          "texture": "textures/ui/Left_Default",
          "size": [ 81 , 34 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "offset": [ -10, 5 ],
          "layer": 10,
          "ignored": "(not $default_state)",
          "uv_size": [ 81 , 34 ],
          "uv": "@start.animation",
          "disable_anim_fast_forward": true
        }
      },
      {
        "Left_Hover": {
          "type": "image",
