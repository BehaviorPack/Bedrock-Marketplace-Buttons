          "size": [ 81 , 34 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "offset": [ -10, 5 ],
          "layer": 10,
          "ignored": "($default_state)",
          "uv_size": [ 81 , 34 ],
          "uv": "@start.animation",
          "disable_anim_fast_forward": true
        }
      },    

      {
        "Right_Default": {
          "type": "image",
          "texture": "textures/ui/Right_Default",
          "size": [ 81 , 34 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "offset": [ 69, 5 ],
          "layer": 10,
          "ignored": "(not $default_state)",
          "uv_size": [ 81 , 34 ],
          "uv": "@start.animation",
          "disable_anim_fast_forward": true
        }
      },
      {
        "Right_Hover": {
          "type": "image",
          "texture": "textures/ui/Right_Hover",
          "size": [ 81 , 34 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "offset": [ 69, 5 ],
          "layer": 10,
          "ignored": "($default_state)",
          "uv_size": [ 81 , 34 ],
          "uv": "@start.animation",
          "disable_anim_fast_forward": true
        }
      },
      
      {
        "Dragon_Default": {
          "type": "image",
          "texture": "textures/ui/Dragon_Default",
          "size": [ 126 , 34 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "offset": [ -4, 5 ],
          "layer": 55,
          "ignored": "(not $default_state)",
